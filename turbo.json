{
   "$schema": "https://turborepo.com/schema.json",
   "ui": "stream",
   "tasks": {
      "build": {
         "dependsOn": ["^build"],
         "inputs": ["$TURBO_DEFAULT$", ".env*"],
         "outputs": [
            "dist/**",
            ".vite/deps/**",
            "build/**",
            ".cache/tsbuildinfo.json"
         ]
      },
      "dev": {
         "cache": false,
         "persistent": true
      },
      "typecheck": {
         "dependsOn": ["^build"]
      }
   }
}
